#include<iostream>
#include<cstdlib>
#include<time.h>
using namespace std;
int main()
{
    //srand(time(NULL));
    //int i_wejsc;
    //cout<<"Podaj ilosc wejsc: "<<endl;
    //cin>>i_wejsc;
    //i_wejsc=rand()%5+1;
    float A=0,B=0,C=0,delta;
    //for (int i=1; i<=i_wejsc; i++)
    //{
    /*cout<<"Podaj A: "<<endl;
    cin>>A;
    cout<<"Podaj B: "<<endl;
    cin>>B;
    cout<<"Podaj C: "<<endl;
    cin>>C;
    */
    while(cin>>A>>B>>C)
    {

        //cout<<"Podane A wynosi: "<<A<<endl;
        //cout<<"Podane B wynosi: "<<B<<endl;
        //cout<<"Podane C wynosi: "<<C<<endl;
        delta=(B*B)-(4*A*C);
        //cout<<"delta dla: "<<i<<" przykladu wynosi: "<<delta[i]<<endl;
        if(delta>0)
            cout<<"2"<<endl;
        else if(delta==0)
            cout<<"1"<<endl;
        else
            cout<<"0"<<endl;
    }
    return 0;
}
