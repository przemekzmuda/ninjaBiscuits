#include<iostream>
#include<cstdlib>
#include<time.h>
using namespace std;
int main()
{
    srand(time(NULL));
    int i_wejsc;
    i_wejsc=rand()%5+1;
    float A,B,C,delta[i_wejsc];
    for (int i=1; i<=i_wejsc; i++)
    {
        /*cout<<"Podaj A: "<<endl;
        cin>>A;
        cout<<"Podaj B: "<<endl;
        cin>>B;
        cout<<"Podaj C: "<<endl;
        cin>>C;
        */
        cin>>A>>B>>C;
        if (A!=0)
        {
        {
            delta[i]=B*B-4*A*C;
        }
        for (int j=1; j<=i_wejsc; j++)
        {
            if(delta[j]>0)
                cout<<'2'<<endl;
            else if(delta[j]==0)
                cout<<'1'<<endl;
            else
                cout<<'0'<<endl;
        }
    }
    else return 0;
    }
    return 0;
}
